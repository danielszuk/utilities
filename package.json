{
  "name": "@danielszuk/utilities",
  "version": "1.0.3",
  "description": "Commonly used utilities for Typescript/Javascript projects.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json && prettier --check \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielszuk/utilities.git"
  },
  "keywords": [
    "utilities",
    "utility",
    "util"
  ],
  "author": "Daniel Szuk <daniel.szuk@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danielszuk/utilities/issues"
  },
  "homepage": "https://github.com/danielszuk/utilities#readme",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
